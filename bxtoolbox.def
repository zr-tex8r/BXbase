  \bxtb@let@name\bxtb@tmpb#2%
  \bxtb@render@prefix
  \expandafter#1\csname \bxtb@tmpb\space \endcsname}
\fi                 %</etex>

%% \bxtb@robustlet\CSA\CSB
\ifbxineTeX         %<*etex>
\def\bxtb@robustlet#1#2{%
