  \bxCheckForMovingArgForTest{%
    \begingroup\expandafter\expandafter\expandafter\endgroup
    \expandafter\ifx\csname #1\endcsname\relax
      \expandafter\@firstoftwo
    \else \expandafter\@secondoftwo
