\def\bxtb@ifprimitive#1{%
  \edef\bxtb@tmpa{\string#1}\edef\bxtb@tmpb{\meaning#1}%
  \ifx\bxtb@tmpa\bxtb@tmpb \expandafter\@firstoftwo
  \else \expandafter\@secondoftwo \fi}

