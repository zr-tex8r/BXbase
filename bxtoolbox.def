  \cspreto \csgpreto \csepreto \csxpreto
  \newbool \providebool \booltrue \boolfalse              %3.5.1
  \setbool \ifbool \notbool
  \newtoggle \providetoggle \toggletrue \togglefalse      %3.5.2
  \settoggle \iftoggle \nottoggle
  \ifdef \ifundef                                         %3.6.1
